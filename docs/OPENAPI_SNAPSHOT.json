  "components": {
    "schemas": {
      "AISignal": {
        "properties": {
          "symbol": {
            "type": "string",
            "title": "Symbol"
          },
          "side": {
            "type": "string",
            "title": "Side"
          },
          "confidence": {
            "type": "number",
            "title": "Confidence"
          },
          "entry": {
            "type": "number",
            "title": "Entry"
          },
          "target": {
            "type": "number",
            "title": "Target"
          },
          "stop_loss": {
            "type": "number",
            "title": "Stop Loss"
          },
          "timeframe": {
            "type": "string",
            "title": "Timeframe"
          },
          "comment": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Comment"
          }
        },
        "type": "object",
        "required": [
          "symbol",
          "side",
          "confidence",
          "entry",
          "target",
          "stop_loss",
          "timeframe"
        ],
        "title": "AISignal"
      },
      "AssistantAction": {
        "properties": {
          "type": {
            "type": "string",
            "title": "Type"
          },
          "label": {
            "type": "string",
            "title": "Label"
          },
          "payload": {
            "type": "object",
            "title": "Payload"
          }
        },
        "type": "object",
        "required": [
          "type",
          "label"
        ],
        "title": "AssistantAction"
      },
      "AssistantRequest": {
        "properties": {
          "messages": {
            "items": {
              "$ref": "#/components/schemas/ChatMessage"
            },
            "type": "array",
            "title": "Messages"
          },
          "context": {
            "$ref": "#/components/schemas/TradingContext"
          }
        },
        "type": "object",
        "title": "AssistantRequest"
      },
      "AssistantResponse": {
        "properties": {
          "answer": {
            "type": "string",
            "title": "Answer"
          },
          "actions": {
            "items": {
              "$ref": "#/components/schemas/AssistantAction"
            },
            "type": "array",
            "title": "Actions"
          }
        },
        "type": "object",
        "required": [
          "answer"
        ],
        "title": "AssistantResponse"
      },
      "Candle": {
        "properties": {
          "time": {
            "type": "integer",
            "title": "Time"
          },
          "open": {
            "type": "number",
            "title": "Open"
          },
          "high": {
            "type": "number",
            "title": "High"
          },
          "low": {
            "type": "number",
            "title": "Low"
          },
          "close": {
            "type": "number",
            "title": "Close"
          },
          "volume": {
            "type": "number",
            "title": "Volume"
          }
        },
        "type": "object",
        "required": [
          "time",
          "open",
          "high",
          "low",
          "close",
          "volume"
        ],
        "title": "Candle"
      },
      "ChatMessage": {
        "properties": {
          "role": {
            "type": "string",
            "enum": [
              "system",
              "user",
              "assistant",
              "tool"
            ],
            "title": "Role"
          },
          "content": {
            "type": "string",
            "title": "Content"
          },
          "name": {
            "anyOf": [
              {
                "type": "string"
